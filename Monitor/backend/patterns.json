{
  "munin-path": "/var/www/html/munin/liberouter.org/collector-nemea.liberouter.org/",
  "patterns": [
    {
      "title": "Traffic monitoring",
      "pattern": "traffic-monitoring/.*_(flows|packets|bytes)",
      "enabled": "true"
    },
    {
      "title": "Nemea",
      "pattern": "nemea_supervisor/nemea_.*",
      "enabled": "true"
    },
    {
      "title": "System status",
      "pattern": "([^/]*)/.*",
      "enabled": "true"
    },
    {
      "title": "Diskstats iops",
      "pattern": "diskstats_iops/.*",
      "enabled": "true"
    }
  ]
}
